# figure out a way to integrate this
function find-in-src {
  # XXX does not handle case-insensitivity
  PATTERN=$1
  SRC_BASE=$HOME/src
  # could combine these, but better to separate by level
  ls -ld $SRC_BASE/*${PATTERN}*      2> /dev/null
  ls -ld $SRC_BASE/*/*${PATTERN}*    2> /dev/null
  ls -ld $SRC_BASE/*/*/*${PATTERN}*  2> /dev/null
}





